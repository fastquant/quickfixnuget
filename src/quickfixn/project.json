{
  "buildOptions": {
    "outputName": "QuickFix",
    "compile": {
      "exclude": [
        "QuickFIXn/Message/FIX50SP1/*.cs",
        "QuickFIXn/Message/FIX50SP2/*.cs"
      ]
    },
    "copyToOutput": {
      "include": [ "spec/**/*" ]
    }
  },
  "packhOptions": {
    "include": [ "spec" ]
  },
  "dependencies": {
    "NETStandard.Library": "1.6.1" 
  },
  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    },
    "netstandard1.3": {
      "System.Xml.XPath": "4.3.0",
      "System.Runtime.Serialization.Xml": "4.3.0",
      "System.Xml.XmlDocument": "4.3.0",
      "System.Xml.XPath.XmlDocument": "4.3.0",
      "System.Runtime.Serialization.Primitives": "4.3.0",
      "System.Threading.Thread": "4.3.0"
    } 
  },
  "version": "1.7.0.020161006",
  "name": "quickfixn"
}
